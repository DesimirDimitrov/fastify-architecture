    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.2.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@prisma/client": "^4.8.1",
    "@sinclair/typebox": "^0.25.20",
